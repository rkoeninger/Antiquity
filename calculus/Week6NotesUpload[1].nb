Cell[CellGroupData[{

Cell[BoxData[
    \(yprimeEq = D[eqn[x, y[x]], x]\)], "Input"],

Cell[BoxData[
    RowBox[{
      RowBox[{"2", " ", \((x\^2 + 2\ y[x]\^2)\), " ", 
        RowBox[{"(", 
          RowBox[{\(2\ x\), "+", 
            RowBox[{"4", " ", \(y[x]\), " ", 
              RowBox[{
                SuperscriptBox["y", "\[Prime]",
                  MultilineFunction->None], "[", "x", "]"}]}]}], ")"}]}], 
      "\[Equal]", 
      RowBox[{"1", "-", \(3\ y[x]\), "+", 
        RowBox[{
          SuperscriptBox["y", "\[Prime]",
            MultilineFunction->None], "[", "x", "]"}], "-", 
        RowBox[{"3", " ", "x", " ", 
          RowBox[{
            SuperscriptBox["y", "\[Prime]",
              MultilineFunction->None], "[", "x", "]"}]}]}]}]], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(sol = Solve[yprimeEq, \(y'\)[x]] // Flatten\)], "Input"],

Cell[BoxData[
    RowBox[{"{", 
      RowBox[{
        RowBox[{
          SuperscriptBox["y", "\[Prime]",
            MultilineFunction->None], "[", "x", "]"}], 
        "\[Rule]", \(\(1 - 4\ x\^3 - 3\ y[x] - 8\ x\ y[x]\^2\)\/\(\(-1\) + 
              3\ x + 8\ x\^2\ y[x] + 16\ y[x]\^3\)\)}], "}"}]], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(dydx = \(\(y'\)[x] /. sol\) /. y[x] \[Rule] y\)], "Input"],

Cell[BoxData[
    \(\(1 - 4\ x\^3 - 3\ y - 8\ x\ y\^2\)\/\(\(-1\) + 3\ x + 8\ x\^2\ y + 16\ \
y\^3\)\)], "Output"]
}, Open  ]],

Cell[BoxData[
    \(Similarly, \ 
    we\ can\ find\ \[DifferentialD]x\/\[DifferentialD]y = \(1 - 3\ x - 8\ \
x\^2\ y - 16\ y\^3\)\/\(\(-1\) + 4\ x\^3 + 3\ y + 8\ x\ y\^2\)\ by\ the\ \
following\ code\)], "Text"],

Cell[CellGroupData[{

Cell[BoxData[{
    \(\(yprimeEq = D[eqn[x[y], y], y];\)\), "\n", 
